  "comparison_algorithms": ["extend","slalom"],
  "budgets": {
    "training": false,
    "validation_and_testing": [500, 1000, 1500, 2000, 2500, 3000, 4000, 5000, 6000, 8000, 10000],
    "validation": [500, 1000, 1500, 2000, 3000, 5000, 7000, 10000]
  },
  "column_filters": {
    "TableNumRowsFilter": 0
  },
  "used_tables": {
    "names": ["lineorder"]
  }
